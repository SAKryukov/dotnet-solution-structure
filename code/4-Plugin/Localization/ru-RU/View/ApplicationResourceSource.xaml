            <system:String x:Key="treeViewItem.Copy/Delete">Копирование/Удаление</system:String>
            <system:String x:Key="treeViewItem.Refresh">Автообновление</system:String>
            <system:String x:Key="treeViewItem.QuickSearch">Быстрый поиск</system:String>            
        </ResourceDictionary>
