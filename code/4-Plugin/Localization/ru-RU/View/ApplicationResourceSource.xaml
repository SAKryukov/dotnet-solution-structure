        xmlns:system="clr-namespace:System;assembly=netstandard"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <FrameworkContentElement.Resources>
        <ResourceDictionary>
            <x:Array x:Key="arrayDemo" Type="system:String">
                <system:String>Демострация:</system:String>
                <system:String>Resource markup extension x:Array</system:String>
                <system:String>в данном случае используется в качестве ListBox.ItemSource</system:String>
            </x:Array>
            <system:String x:Key="menuItem.Quit">Выход</system:String>
            <system:String x:Key="menuItem.Quit.Icon">👌</system:String>
            <system:String x:Key="treeViewItem.Operation">Основные операции</system:String>
