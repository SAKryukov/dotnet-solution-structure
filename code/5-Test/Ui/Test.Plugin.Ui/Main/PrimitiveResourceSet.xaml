<ContentPresenter x:Class="SA.Test.Main.PrimitiveResourceSet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:system="clr-namespace:System;assembly=netstandard"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ContentPresenter.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <system:String x:Key="definitionSet.DialogPropertySet.PluginDialogTitle"> Load and Execute Plugin</system:String>
                    <system:String x:Key="definitionSet.DialogPropertySet.PluginDialogFilter"> Plugin files|Plugin*.dll</system:String>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </ContentPresenter.Resources>
</ContentPresenter>
