<FrameworkContentElement x:Class="SA.Test.Extensions.ResourceSource"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:System="clr-namespace:System;assembly=netstandard"
        xmlns:Ext="clr-namespace:SA.Agnostic.IO.Extensions"
        xmlns:Test="clr-namespace:SA.Test.Extensions"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <FrameworkContentElement.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <Ext:KeyedMember x:Key="{Ext:EKey TargetType={x:Type Test:ResourceTarget}}"
                         Name="Name"
                         MemberKind="Field"
                         Value="My name"/>
                    <Ext:KeyedMember x:Key="{Ext:EKey TargetType={x:Type Test:ResourceTarget}}"
                         Name="Count"
                         Type="{x:Type System:Int32}"
                         MemberKind="Property"
                         Value="13"/>
                    <Ext:KeyedMember x:Key="{Ext:EKey TargetType={x:Type Test:ResourceTarget}}"
                         Name="Description"
                         MemberKind="Property"
                         Value="This is a demonstration of getting values from a resource dictionary"/>
                </ResourceDictionary>
                <ResourceDictionary>
                    <Ext:Member x:Key="Name"
                         MemberKind="Field"
                         Value="My family name"/>
                    <!-- Below are all properties, default: -->
                    <Ext:Member x:Key="Count"
                         Type="{x:Type System:Int32}"
                         Value="100"/>
                    <Ext:Member x:Key="Description"
                         Value="This is a simpler demonstration of getting values from a resource dictionary"/>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </FrameworkContentElement.Resources>
</FrameworkContentElement>
    