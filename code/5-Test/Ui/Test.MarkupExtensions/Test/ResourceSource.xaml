<FrameworkContentElement x:Class="SA.Test.Extensions.ResourceSource"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:System="clr-namespace:System;assembly=netstandard"
        xmlns:Ext="clr-namespace:SA.Test.Extensions.Declarations"
        xmlns:Test="clr-namespace:SA.Test.Extensions"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <FrameworkContentElement.Resources>
        <Ext:Member x:Key="{Ext:EKey TargetType={x:Type Test:ResourceTarget}}"
                         Name="Name"
                         MemberKind="Field"
                         Value="My name"/>
        <Ext:Member x:Key="{Ext:EKey TargetType={x:Type Test:ResourceTarget}}"
                         Name="Count"
                         Type="{x:Type System:Int32}"
                         MemberKind="Property"
                         Value="13"/>
        <Ext:Member x:Key="{Ext:EKey TargetType={x:Type Test:ResourceTarget}}"
                         Name="Description"
                         MemberKind="Property"
                         Value="This is the demonstration of getting values from a resource dictionary"/>
    </FrameworkContentElement.Resources>
</FrameworkContentElement>
    